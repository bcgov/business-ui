{
  "name": "person-roles",
  "version": "0.0.30",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "build-check": "nuxt build",
    "build": "nuxt generate",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest --dir tests/unit",
    "test:unit": "vitest --dir tests/unit",
    "test:unit:cov": "vitest --dir tests/unit --coverage",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "build:test": "pnpm generate && npx serve .output/public"
  },
  "dependencies": {
    "nuxt": "^4.0.3",
    "vue": "^3.5.20",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@faker-js/faker": "^10.0.0",
    "@types/node": "^24.0.10",
    "typescript": "^5.8.3",
    "vue-tsc": "^3.0.4"
  }
}
